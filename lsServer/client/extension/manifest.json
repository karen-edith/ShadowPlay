{
  "name":"Desktop Capture",
  "description":"allows you to capture you desktop for use in application",
  "version": "0.1.0",
  "manifest_version": 2,
  "background":{
    "scripts":["extension.js"],
    "persistent": false
  },
  "externally_connectable":{
    "matches":["*://localhost/*"]
  },
  "permissions":["desktopCapture"]
}
